//11721 열 개씩 끊어 출력하기
#include <iostream>
#include <string>
using namespace std;
int main(void){
    string input;
    cin>>input;
    string::iterator it = input.begin();
    while(it!=input.end()){
	    for(int i=0; i<10 && it != input.end(); i++){
	        cout<<*it;
	        it++;
	    }
	    cout<<endl;
    }
    
    return 0;
}
